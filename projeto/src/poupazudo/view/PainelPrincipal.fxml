<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<Pane onMouseEntered="#carregarCampos" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poupazudo.controller.PainelPrincipalController">
   <children>
      <ImageView fitHeight="53.0" fitWidth="54.0" layoutX="11.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../res/pig.png" />
         </image>
      </ImageView>
      <Label fx:id="lbNomeUsuario" layoutX="72.0" layoutY="9.0" text="Nome do Usuario">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="lbEmailUsuario" layoutX="72.0" layoutY="26.0" text="email@gmail.com" textFill="#abaaaa">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <Separator layoutX="216.0" orientation="VERTICAL" prefHeight="500.0" />
      <Pane layoutX="223.0" prefHeight="500.0" prefWidth="581.0">
         <children>
            <TabPane layoutX="-1.0" prefHeight="500.0" prefWidth="581.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Contas">
                     <content>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Hyperlink fx:id="hlAdicionarConta" layoutX="4.0" layoutY="15.0" onAction="#gotoAdicionarConta" text="Adicionar Conta" />
                              <TableView fx:id="tvListaContas" layoutX="6.0" layoutY="60.0" prefHeight="351.0" prefWidth="304.0">
