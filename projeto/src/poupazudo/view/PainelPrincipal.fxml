                  </Tab>
                  <Tab text="Estatísticas">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab text="Relatórios">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </Pane>
      <Hyperlink fx:id="hlSair" layoutX="68.0" layoutY="47.0" onAction="#gotoConfirmarSair" text="Encerrar sessão" />
