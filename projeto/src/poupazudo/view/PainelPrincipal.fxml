                           </children>
                        </Pane>
                     </content>
                  </Tab>
                <Tab text="Receita">
                     <content>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Hyperlink fx:id="hlAdicionarReceita" layoutX="4.0" layoutY="15.0" onAction="#gotoCriarReceita" text="Adicionar Receita" />
                           </children>
                        </Pane>
