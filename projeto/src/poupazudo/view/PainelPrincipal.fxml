                                    <TableColumn fx:id="tcNome" prefWidth="152.0" text="Conta" />
                                    <TableColumn fx:id="tcSaldo" prefWidth="145.0" text="Saldo">
                                       <columns>
                                          <TableColumn fx:id="tcSaldoPrevisto" prefWidth="75.0" text="Previsto" />
                                          <TableColumn fx:id="tcSaldoAtual" prefWidth="75.0" text="Atual" />
                                       </columns>
                                    </TableColumn>
                                 </columns>
                              </TableView>
                           </children>
                        </Pane>
                     </content>
                  </Tab>
                <Tab text="Despesa">
                     <content>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Hyperlink fx:id="hlAdicionarDespesa" layoutX="4.0" layoutY="15.0" onAction="#gotoCriarDespesa" text="Adicionar Despesa" />
                           </children>
                        </Pane>
                     </content>
                  </Tab>
                <Tab text="Receita">
                     <content>
                        <Pane prefHeight="200.0" prefWidth="200.0">
                           <children>
                              <Hyperlink fx:id="hlAdicionarReceita" layoutX="4.0" layoutY="15.0" onAction="#gotoCriarReceita" text="Adicionar Receita" />
                           </children>
                        </Pane>
                     </content>
                  </Tab>
                  <Tab text="Metas e Orçamentos">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="349.0" prefWidth="377.0" />
                    </content>
                  </Tab>
                  <Tab text="Extrato">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab text="Calendário">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab text="Estatísticas">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
                  <Tab text="Relatórios">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </Pane>
      <Hyperlink fx:id="hlSair" layoutX="68.0" layoutY="42.0" onAction="#gotoConfirmarSair" text="Sair" />
      <Label layoutX="16.0" layoutY="386.0" text="Saldo Total">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="16.0" layoutY="405.0" text="R$">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lbValorSaldo" layoutX="42.0" layoutY="390.0" prefHeight="70.0" prefWidth="168.0" text="0,00">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Rectangle arcHeight="5.0" fill="DODGERBLUE" height="53.0" layoutX="-14.0" layoutY="391.0" smooth="false" stroke="#1e90fff2" strokeType="INSIDE" width="20.0" />
      <Label layoutX="16.0" layoutY="124.0" text="Atividades Recentes">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</Pane>
